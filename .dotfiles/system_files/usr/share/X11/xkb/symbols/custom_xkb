xkb_symbols "basic"  {
	include "eu(basic)";
    name[Group1] = "EurKEY (US) (customized)";

    key <AC10>	{[        colon,       semicolon,               degree,       periodcentered  ]};

    include "level3(ralt_switch)"
};
